{
  "name": "lezer-feel",
  "version": "0.5.1",
  "description": "lezer-based FEEL parser and evaluator",
  "main": "dist/index.js",
  "author": "Nico Rehwaldt <git_nikku@nixis.de>",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "esm": "^3.2.25",
    "fast-glob": "^3.1.1",
    "lezer-generator": "^0.5.0",
    "mocha": "^6.1.3",
    "rollup": "^1.10.0",
    "rollup-plugin-terser": "^5.1.3",
    "strip-indent": "^3.0.0"
  },
  "dependencies": {
    "lezer": "^0.5.0"
  },
  "scripts": {
    "all": "npm run build && npm run test",
    "build-parser:watch": "chokidar --initial src/feel.grammar -c \"npm run build-parser -- --names\"",
    "build-parser": "lezer-generator src/feel.grammar -o src/parser --names",
    "build": "rollup -c",
    "prebuild": "npm run build-parser",
    "prepare": "npm run build",
    "test": "mocha -r esm test/*.js",
    "test:watch": "npm test -- -w"
  }
}
