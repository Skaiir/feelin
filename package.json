{
  "name": "feelin",
  "version": "0.23.0",
  "description": "A FEEL parser and interpreter",
  "main": "dist/index.js",
  "author": "Nico Rehwaldt <git_nikku@nixis.de>",
  "license": "MIT",
  "type": "module",
  "module": "dist/index.esm.js",
  "umd:main": "dist/feelin.umd.js",
  "source": "src/index.ts",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.16.0",
    "eslint-plugin-bpmn-io": "^0.11.0",
    "fast-glob": "^3.2.4",
    "mkdirp": "^1.0.4",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.35.1",
    "rollup-plugin-terser": "^7.0.2",
    "saxen": "^8.1.2",
    "strip-indent": "^3.0.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "lezer": "^0.9.0",
    "lezer-feel": "0.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/feelin"
  },
  "sideEffects": false,
  "scripts": {
    "all": "run-s lint build generate-typings test",
    "lint": "eslint .",
    "build": "rollup -c",
    "generate-typings": "tsc src/index.ts --emitDeclarationOnly --declaration --outDir dist",
    "prepare": "run-s build",
    "test": "mocha test/*-spec.js",
    "test:watch": "npm test -- -w",
    "tck": "run-s tck:extract tck:test",
    "tck:extract": "node tasks/extract-tck-tests.js ../dmn-tck",
    "tck:test": "mocha test/tck/*.js"
  }
}
