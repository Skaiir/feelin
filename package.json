{
  "name": "feelin",
  "version": "0.18.0",
  "description": "A FEEL parser and interpreter",
  "main": "dist/index.js",
  "author": "Nico Rehwaldt <git_nikku@nixis.de>",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "umd:main": "dist/feelin.umd.js",
  "source": "src/index.js",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-bpmn-io": "^0.10.0",
    "esm": "^3.2.25",
    "fast-glob": "^3.1.1",
    "lezer-generator": "^0.7.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.10.0",
    "rollup-plugin-terser": "^5.1.3",
    "saxen": "^8.1.0",
    "strip-indent": "^3.0.0"
  },
  "dependencies": {
    "lezer": "^0.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/feelin"
  },
  "sideEffects": false,
  "scripts": {
    "all": "run-s lint build-parser build test",
    "lint": "eslint .",
    "build-parser": "run-s build-parser:*",
    "build-parser:expr:watch": "chokidar --initial src/grammar/expression.grammar -c \"npm run build-parser:expr -- --names\"",
    "build-parser:expr": "lezer-generator src/grammar/expression.grammar -o src/grammar/expression-parser",
    "build-parser:unary:watch": "chokidar --initial src/grammar/unary-test.grammar -c \"npm run build-parser:unary -- --names\"",
    "build-parser:unary": "lezer-generator src/grammar/unary-test.grammar -o src/grammar/unary-test-parser",
    "build": "rollup -c",
    "prepublishOnly": "run-s build-parser build",
    "test": "mocha -r esm test/*.js",
    "test:watch": "npm test -- -w",
    "tck": "run-s tck:extract tck:test",
    "tck:extract": "node tasks/extract-tck-tests.js ../dmn-tck",
    "tck:test": "mocha -r esm test/tck/*.js",
    "grammars": "mocha -r esm test/grammar/*.js"
  }
}
